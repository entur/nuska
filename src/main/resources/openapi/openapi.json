{
  "openapi": "3.1.0",
  "info": {
    "title": "Timetable data API",
    "description": "Provide access to the original NeTEx datasets uploaded by data providers",
    "version": "1.0"
  },
  "servers": [
    {
      "url": "https://api.dev.entur.io/timetable/v1/timetable-data",
      "description": "Development"
    },
    {
      "url": "https://api.staging.entur.io/timetable/v1/timetable-data",
      "description": "Staging"
    },
    {
      "url": "https://api.entur.io/timetable/v1/timetable-data",
      "description": "Production"
    }
  ],
  "security": [
    {
      "jwt": []
    }
  ],
  "tags": [
    {
      "name": "TimetableData",
      "description": "Give access to the original NeTEx datasets delivered by data providers"
    }
  ],
  "paths": {
    "/timetable-data/{codespace}": {
      "get": {
        "tags": [
          "TimetableData"
        ],
        "description": "Return the dataset identified by the given codespace and import key",
        "operationId": "downloadTimetableData",
        "parameters": [
          {
            "name": "codespace",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "importKey",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "Accept",
            "in": "header",
            "required": false,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "*/*": {
                "schema": {
                  "type": "string",
                  "format": "binary"
                }
              }
            }
          }
        },
        "deprecated": true
      }
    },
    "/timetable-data/datasets/{codespace}/versions": {
      "get": {
        "tags": [
          "TimetableData"
        ],
        "description": "List the import keys for the latest imported datasets for a given codespace",
        "operationId": "getDatasetVersions",
        "parameters": [
          {
            "name": "codespace",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/NetexImport"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/timetable-data/datasets/{codespace}/version/{importKey}": {
      "get": {
        "tags": [
          "TimetableData"
        ],
        "description": "Return the dataset identified by the given codespace and import key",
        "operationId": "getDataset",
        "parameters": [
          {
            "name": "codespace",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "importKey",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "Accept",
            "in": "header",
            "required": false,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/octet-stream": {
                "schema": {
                  "type": "string",
                  "format": "binary"
                }
              }
            }
          }
        }
      }
    },
    "/timetable-data/datasets/{codespace}/latest": {
      "get": {
        "tags": [
          "TimetableData"
        ],
        "description": "Return the latest dataset for a given codespace",
        "operationId": "getLatestDataset",
        "parameters": [
          {
            "name": "codespace",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "Accept",
            "in": "header",
            "required": false,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/octet-stream": {
                "schema": {
                  "type": "string",
                  "format": "binary"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "NetexImport": {
        "type": "object",
        "properties": {
          "importKey": {
            "type": "string"
          },
          "creationDate": {
            "type": "string",
            "format": "date-time"
          }
        }
      }
    },
    "securitySchemes": {
      "jwt": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    }
  }
}